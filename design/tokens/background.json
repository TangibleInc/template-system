{
  "bg-attachment": {"col": 1, "data": ["bg-fixed", "bg-local", "bg-scroll"]},

  "bg-clip": {
    "col": 1,
    "data": [
      "bg-clip-border",
      "bg-clip-padding",
      "bg-clip-content",
      "bg-clip-text"
    ]
  },
  "[bg-color|gradient-Color-Stops]": {
    "col": 1,
    "data": [
      "[bg|from]-transparent",
      "[bg|from]-[inherit|current]",
      "[bg|from]-[white|black]",
      "[bg|from]-[$color]-[50-900]"
    ]
  },
  "bg-position": {
    "col": 1,
    "data": [
      "bg-center",
      "bg-[left|right|top|bottom]",
      "bg-[left|right]-[top|bottom]"
    ]
  },

  "bg-image": {
    "col": 2,
    "data": [
      "bg-none",
      "bg-gradient-to-[t|r|b|l]",
      "bg-gradient-to-[tl|tr|bl|br]"
    ]
  },
  "bg-repeat": {
    "col": 2,
    "data": [
      "bg-repeat",
      "bg-norepeat",
      "bg-repeat-[x|y]",
      "bg-repeat-round",
      "bg-repeat-space"
    ]
  },
  "bg-size": {"col": 2, "data": ["bg-auto", "bg-cover", "bg-contain"]},
  "bg-origin": {
    "col": 2,
    "data": ["bg-origin-border", "bg-origin-padding", "bg-origin-content"]
  }
}
