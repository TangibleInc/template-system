      "rounded-[sm|md|lg|xl|2xl|3xl]",
      "rounded-[full|none]",
      "rounded-[t|r|b|l]",
      "rounded-[t|r|b|l]-[full|none]",
      "rounded-[t|r|b|l]-[sm|md|lg|xl|2xl|3xl]",
      "rounded-[tr|tl|br|bl]",
      "rounded-[tr|tl|br|bl]-[full|none]",
      "rounded-[tr|tl|br|bl]-[sm|md|lg|xl|2xl|3xl]"
    ]
