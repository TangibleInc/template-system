{
  "Family": {"col": 1, "data": ["font-sans", "font-serif", "font-mono"]},
  "size": {
    "col": 1,
    "data": ["text-[xs|sm|lg|xl]", "text-base", "text-[2-9]xl"]
  },
  "smoothing": {"col": 4, "data": ["antialiased", "subpixel-antialiased"]},
  "style": {"col": 2, "data": ["italic", "not-italic"]},
  "weight": {
    "col": 2,
    "data": [
      "font-[thin|extralight|light]",
      "font-normal",
      "font-medium",
      "font-[semibold|bold|extrabold]",
      "font-black"
    ]
  },
  "font-variant-numeric": {
    "col": 2,
    "data": [
      "ordinal",
      "slashed-zero",
      "[normal|lining|tabular]-nums",
      "[oldstyle|proportional]-nums",
      "[diagonal|stacked]-fractions"
    ]
  },
  "letter-spacing": {
    "col": 2,
    "data": [
      "tracking-[tight|tighter]",
      "tracking-normal",
      "tracking-[wide|wider|widest]"
    ]
  },
  "line-height": {
    "col": 1,
    "data": [
      "leading-[3-10]",
      "leading-none",
      "leading-tight",
      "leading-snug",
      "leading-normal",
      "leading-relaxed",
      "leading-loose"
    ]
  },
  "list-style-type": {
    "col": 4,
    "data": ["list-none", "list-disc", "list-decimal"]
  },
  "list-style-positions": {"col": 4, "data": ["list-inside", "list-outside"]},
  "text-align": {
    "col": 1,
    "data": [
      "text-[left|right]",
      "text-center",
      "text-justify",
      "text-[start|end]"
    ]
  },
  "[placeholder|text]-color": {
    "col": 2,
    "data": [
      "[placeholder|text]-transparent",
      "[placeholder|text]-current",
      "[placeholder|text]-[white|black]",
      "[placeholder|text]-[$COLOR]-[50-900]"
    ]
  },
  "text-indent": {"col": 3, "data": ["indent-[$SPACING]"]},
  "text-opacity": {
    "col": 3,
    "data": ["text-opacity-[$OPACITY]"]
  },
  "text-decoration": {
    "col": 3,
    "data": ["underline", "no-underline", "line-through", "overline"]
  },
  "text-decoration-color": {
    "col": 3,
    "data": [
      "decoration-inherit",
      "decoration-transparent",
      "decoration-current",
      "decoration-[white/black]",
      "decoration-[$COLOR]-[50-900]"
    ]
  },
  "text-decoration-style": {
    "col": 3,
    "data": [
      "decoration-solid",
      "decoration-double",
      "decoration-dashed",
      "decoration-dotted",
      "decoration-wavy"
    ]
  },
  "text-decoration-thickness": {
    "col": 3,
    "data": [
      "decoration-auto",
      "decoration-from-font",
      "decoration-[0|1|2|4|8]"
    ]
  },
  "text-underline-offset": {
    "col": 3,
    "data": ["underline-offset-auto", "underline-offset-[0|1|2|4|8]"]
  },
  "text-transform": {
    "col": 1,
    "data": ["uppercase", "lowercase", "capitalize", "normal-case"]
  },
  "text-overflow": {
    "col": 1,
    "data": ["truncate", "text-ellipsis", "text-clip"]
  },
  "vertical-align": {
    "col": 2,
    "data": [
      "align-baseline",
      "align-[top|middle|bottom]",
      "align-text-[top|bottom]",
      "align-[sub|super]"
    ]
  },
  "white-space": {
    "col": 4,
    "data": [
      "whitespacing-normal",
      "whitespacing-no-wrap",
      "whitespacing-pre",
      "whitespacing-pre-[line|wrap]"
    ]
  },
  "word-break": {
    "col": 4,
    "data": ["break-normal", "break-words", "break-all", "break-keep"]
  },
  "content": {"col": 4, "data": ["content-none"]}
}
