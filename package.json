{
  "name": "tangible-template-system",
  "description": "Template System module",
  "homepage": "https://bitbucket.org/tangibleinc/template-system",
  "repository": "git@bitbucket.org:tangibleinc/template-system.git",
  "tangible": {
    "deployType": "module"
  },
  "scripts": {
    "-- Run commands below like: npm run build [module name]": "",
    "dev": "roll dev",
    "build": "roll build",
    "format": "roll format",
    "--": "",
    "build:all": "roll build interface; roll build logic; roll build template; roll build tester; roll build system",
    "format:all": "roll format interface; roll format logic; roll format template; roll format tester; roll format system",
    "-- Local environment --": "https://developer.wordpress.org/block-editor/reference-guides/packages/packages-env",
    "env": "wp-env",
    "env:start": "wp-env start",
    "env:test": "wp-env run tests-wordpress /var/www/html/wp-content/plugins/template-system/vendor/bin/phpunit -c /var/www/html/wp-content/plugins/template-system/phpunit.xml --verbose",
    "env:stop": "wp-env stop",
    "env:destroy": "wp-env destroy"
  },
  "devDependencies": {
    "@tangible/roller": "1.x",
    "@wordpress/env": "^5.15.0"
  }
}
